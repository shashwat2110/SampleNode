<html>
    <head>
        <title>EJS View Test</title>
    </head>

    <body>
        <h1><%= title %></h1>

        <h2><%= topic %></h2>

        <h3>
            <%= status %>
            <% if (status == 0) { %>
                Not started, Bitch!
            <% } else if (status == 1) { %>
                It's already On, Biitch!!
            <% } else { %>
                WTF, Biiiiitch!!!
            <% } %>
        </h3>

        <% letter.forEach(function(iter){ %>
            <h4><%= iter %></h4>
        <% }) %>

        <h5><%= error.code %> <%= error.msg %></h5>
    </body>
</html>